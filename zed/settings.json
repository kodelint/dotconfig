{
  "active_pane_modifiers": {
    "border_size": 0.0,
    "inactive_opacity": 0.5
  },
  "auto_install_extensions": {
    "catppuccin": true,
    "csv": true,
    "dockerfile": true,
    "emmet": true,
    "git-firefly": true,
    "html": true,
    "log": true,
    "sql": true,
    "svelte": true,
    "toml": true,
    "unocss": true,
    "vue": true,
    "xml": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "auto_signature_help": true,
  "bottom_dock_layout": "full",
  "buffer_font_features": {
    "calt": true
  },
  "buffer_font_family": "0xProto Nerd Font Propo",
  "buffer_font_size": 14.5,
  "chat_panel": {
    "button": "when_in_call",
    "default_width": 240,
    "dock": "right"
  },
  "collaboration_panel": {
    "button": true,
    "default_width": 240,
    "dock": "left"
  },
  "close_on_file_delete": true,
  "confirm_quit": false,
  "cursor_blink": true,
  "disable_ai": true,
  "double_click_in_multibuffer": "select",
  "file_types": {
    ".env": [
      ".env*"
    ],
    "Dockerfile": [
      "Dockerfile",
      "Dockerfile.*",
      "*.dockerfile"
    ],
    "JSON": [
      "*.json",
      "*.jsonc",
      ".eslintrc",
      ".prettierrc"
    ]
  },
  "format_on_save": "language_server",
  "git": {
    "git_gutter": "tracked_files",
    "gutter_debounce": null,
    "hunk_style": "staged_hollow",
    "inline_blame": {
      "enabled": true,
      "min_column": 80,
      "show_commit_summary": true
    }
  },
  "git_panel": {
    "button": true,
    "dock": "left",
    "default_width": 360,
    "status_style": "icon",
    "fallback_branch_name": "main",
    "sort_by_path": true,
    "collapse_untracked_diff": false,
    "scrollbar": {
      "show": null
    }
  },
  "go_to_definition_fallback": "find_all_references",
  "gutter": {
    "breakpoints": true,
    "folds": true,
    "line_numbers": true,
    "min_line_number_digits": 4,
    "runnables": true
  },
  "hard_tabs": false,
  "helix_mode": true,
  "horizontal_scroll_margin": 3,
  "hover_popover_enabled": true,
  "icon_theme": {
    "dark": "JetBrains New UI Icons (Dark)",
    "light": "Zed (Default)",
    "mode": "dark"
  },
  "indent_guides": {
    "active_line_width": 2,
    "coloring": "indent_aware",
    "enabled": true,
    "line_width": 1
  },
  "inlay_hints": {
    "edit_debounce_ms": 700,
    "enabled": true,
    "scroll_debounce_ms": 50,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true
  },
  "languages": {
    "CSS": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "Go": {
      "allow_rewrap": "in_selections",
      "code_actions_on_format": {
        "source.organizeImports": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "hard_tabs": true,
      "tab_size": 4
    },
    "HTML": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "JSON": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
        "source.organizeImports": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "hard_tabs": false,
      "tab_size": 2
    },
    "Markdown": {
      "format_on_save": "on",
      "preferred_line_length": 80,
      "soft_wrap": "preferred_line_length",
      "tab_size": 2
    },
    "Python": {
      "code_actions_on_format": {
        "source.fixAll.ruff": true,
        "source.organizeImports": true
      },
      "format_on_save": "on",
      "formatter": {
        "external": {
          "arguments": [
            "format",
            "--stdin-filename",
            "{buffer_path}",
            "-"
          ],
          "command": "ruff"
        }
      },
      "tab_size": 4
    },
    "Rust": {
      "allow_rewrap": "in_selections",
      "code_actions_on_format": {
        "source.organizeImports": true
      },
      "enable_language_server": true,
      "format_on_save": "on",
      "formatter": "language_server",
      "indent_guides": null,
      "remove_trailing_whitespace_on_save": true,
      "show_completion_documentation": true,
      "tab_size": 4
    },
    "TSX": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
        "source.organizeImports": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "hard_tabs": false,
      "tab_size": 2
    },
    "TypeScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
        "source.organizeImports": true
      },
      "format_on_save": "on",
      "formatter": "language_server",
      "hard_tabs": false,
      "tab_size": 2
    },
    "YAML": {
      "format_on_save": "on",
      "formatter": "language_server",
      "tab_size": 2
    }
  },
  "lsp": {
    "eslint": {
      "settings": {
        "codeActionOnSave": {
          "rules": [
            "import/order"
          ]
        }
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy"
        },
        "diagnostics": {
          "experimental": {
            "enable": true
          }
        },
        "inlayHints": {
          "bindingModeHints": {
            "enable": true
          },
          "chainingHints": {
            "enable": true
          },
          "closingBraceHints": {
            "enable": true,
            "minLines": 25
          },
          "closureReturnTypeHints": {
            "enable": "never"
          },
          "expressionAdjustmentHints": {
            "enable": "always",
            "hideOutsideUnsafe": true
          },
          "implicitDrops": {
            "enable": true
          },
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "maxLength": 25,
          "parameterHints": {
            "enable": true
          },
          "reborrowHints": {
            "enable": "always"
          },
          "renderColons": true,
          "typeHints": {
            "enable": true,
            "hideClosureInitialization": false,
            "hideNamedConstructor": false
          }
        },
        "procMacro": {
          "enable": true
        }
      }
    },
    "typescript-language-server": {
      "initialization_options": {
        "preferences": {
          "includeInlayEnumMemberValueHints": true,
          "includeInlayFunctionLikeReturnTypeHints": true,
          "includeInlayFunctionParameterTypeHints": true,
          "includeInlayParameterNameHints": "all",
          "includeInlayParameterNameHintsWhenArgumentMatchesName": true,
          "includeInlayPropertyDeclarationTypeHints": true,
          "includeInlayVariableTypeHints": true
        }
      }
    }
  },
  "multi_cursor_modifier": "cmd_or_ctrl",
  "notification_panel": {
    "button": true,
    "default_width": 380,
    "dock": "bottom"
  },
  "on_last_window_closed": "quit_app",
  "outline_panel": {
    "button": true,
    "default_width": 300,
    "dock": "right",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "indent_guides": {
      "show": "always"
    }
  },
  "preferred_line_length": 100,
  "preview_tabs": {
    "enabled": true,
    "enable_preview_from_file_finder": true,
    "enable_preview_from_code_navigation": true
  },
  "project_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "button": true,
    "default_width": 240,
    "dock": "left",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20
  },
  "redact_private_values": false,
  "relative_line_numbers": false,
  "restore_on_startup": "last_workspace",
  "scrollbar": {
    "diagnostics": "none",
    "git_diff": true,
    "search_results": true,
    "show": "auto"
  },
  "scroll_beyond_last_line": "off",
  "scroll_sensitivity": 3.0,
  "search": {
    "button": true,
    "case_sensitive": false,
    "include_ignored": false,
    "regex": false,
    "whole_word": false
  },
  "seed_search_query_from_cursor": "selection",
  "show_completion_documentation": true,
  "show_completions_on_input": true,
  "show_whitespaces": "selection",
  "soft_wrap": "editor_width",
  "tab_bar": {
    "show_nav_history_buttons": true
  },
  "tab_size": 2,
  "tabs": {
    "close_position": "right",
    "file_icons": true,
    "git_status": true,
    "activate_on_close": "left_neighbour"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "font_family": "0xProto Nerd Font",
    "font_size": 15,
    "line_height": "standard",
    "font_features": {
      "calt": true
    },
    "option_as_meta": true
  },
  "theme": {
    "dark": "0x96f Theme",
    "light": "One Light",
    "mode": "dark"
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_onboarding_banner": true,
    "show_project_items": true,
    "show_sign_in": true,
    "show_user_picture": true
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": true,
    "selections_menu": true
  },
  "ui_font_features": {
    "calt": true
  },
  "ui_font_family": "0xProto Nerd Font",
  "ui_font_size": 16.5,
  "unnecessary_code_fade": 0.9,
  "use_auto_surround": true,
  "use_smartcase_search": true,
  "vertical_scroll_margin": 3,
  "vim_mode": false,
  "when_closing_with_no_tabs": "platform_default",
  "wrap_guides": [
    80,
    100
  ]
}
